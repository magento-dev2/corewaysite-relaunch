  "sdks": {
    "title": "Official SDKs",
    "description": "Use our official SDKs for easier integration",
    "libraries": [
      {
        "language": "JavaScript/TypeScript",
        "package": "@coreway/sdk",
        "install": "npm install @coreway/sdk",
        "documentation": "https://docs.corewaysolution.com/sdk/javascript",
        "example": "import { CorewayClient } from '@coreway/sdk';\n\nconst client = new CorewayClient({ apiKey: 'YOUR_API_KEY' });\nconst projects = await client.projects.list();"
      },
      {
        "language": "Python",
        "package": "coreway-sdk",
        "install": "pip install coreway-sdk",
        "documentation": "https://docs.corewaysolution.com/sdk/python",
        "example": "from coreway import CorewayClient\n\nclient = CorewayClient(api_key='YOUR_API_KEY')\nprojects = client.projects.list()"
      },
      {
        "language": "PHP",
        "package": "coreway/sdk",
        "install": "composer require coreway/sdk",
        "documentation": "https://docs.corewaysolution.com/sdk/php",
        "example": "$client = new \\Coreway\\CorewayClient('YOUR_API_KEY');\n$projects = $client->projects->list();"
      },
      {
        "language": "Ruby",
        "package": "coreway-sdk",
        "install": "gem install coreway-sdk",
        "documentation": "https://docs.corewaysolution.com/sdk/ruby",
        "example": "require 'coreway'\n\nclient = Coreway::Client.new(api_key: 'YOUR_API_KEY')\nprojects = client.projects.list"
      }
    ]
  }
}
